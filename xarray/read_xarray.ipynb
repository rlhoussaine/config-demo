    "\n",
    "# Afficher les informations du Dataset\n",
    "print(dataset)\n",
    "\n",
    "# Accéder aux données de température\n",
    "temperature = dataset[\"temperature\"]\n",
    "\n",
    "# Afficher les données de température\n",
    "print(temperature)\n"
   ]
